  <define name="font-settings">
    <element name="font-settings">
      <a:documentation>Settings for fonts</a:documentation>
      <optional>
        <attribute name="auto-fallback-font">
          <choice>
            <value>false</value>
            <value>true</value>
          </choice>
        </attribute>
      </optional>
      <optional>
        <attribute name="barcode-text-font">
          <a:documentation>Font for a linear barcode</a:documentation>
          <choice>
            <value>OCRB</value>
            <value>monospace</value>
          </choice>
        </attribute>
      </optional>
      <optional>
        <attribute name="default-font-family">
          <a:documentation>Default font family</a:documentation>
          <data type="NCName"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="fallback-glyph">
          <a:documentation>Whether, and at what level, to report when a glyph is not found in a fallback font</a:documentation>
          <choice>
            <value>0</value>
            <value>1</value>
            <value>2</value>
          </choice>
        </attribute>
      </optional>
      <optional>
        <attribute name="font-selection-mode">
          <a:documentation>Font selection method: 5 = disregard 'font-selection-strategy'; 6 = act on 'font-selection-strategy="character-by-character"'</a:documentation>
          <choice>
            <value>5</value>
            <value>6</value>
          </choice>
        </attribute>
      </optional>
      <optional>
        <attribute name="font-selection-strategy">
          <data type="NCName"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="font-stretch-mode">
          <data type="integer"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="missing-font">
          <data type="integer"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="missing-glyph">
          <a:documentation>Whether to warn when glyph not found: [ = no message; 1 = level 1 error message; 2 = level 2 error message</a:documentation>
          <choice>
            <value>0</value>
            <value>1</value>
            <value>2</value>
          </choice>
        </attribute>
      </optional>
      <optional>
        <attribute name="missing-glyph-all">
          <choice>
            <value>false</value>
            <value>true</value>
          </choice>
        </attribute>
      </optional>
      <optional>
        <attribute name="use-windows-api">
          <choice>
            <value>false</value>
            <value>true</value>
          </choice>
        </attribute>
      </optional>
      <zeroOrMore>
        <element name="script-font">
          <a:documentation>Generic font mapping for a script</a:documentation>
          <optional>
            <attribute name="cursive">
              <a:documentation>Generic cursive font for script</a:documentation>
            </attribute>
          </optional>
          <optional>
            <attribute name="fallback">
              <a:documentation>Generic fallback font for script</a:documentation>
            </attribute>
          </optional>
          <optional>
            <attribute name="fantasy">
              <a:documentation>Generic fantasy font for script</a:documentation>
            </attribute>
          </optional>
          <optional>
            <attribute name="monospace">
              <a:documentation>Generic monospace font for script</a:documentation>
            </attribute>
          </optional>
          <optional>
            <attribute name="sans-serif">
              <a:documentation>Generic sans-serif font for script</a:documentation>
            </attribute>
          </optional>
          <optional>
            <attribute name="script">
              <a:documentation>Script code</a:documentation>
              <ref name="script.list"/>
            </attribute>
          </optional>
          <optional>
            <attribute name="serif">
              <a:documentation>Generic serif font for script</a:documentation>
            </attribute>
          </optional>
          <empty/>
        </element>
      </zeroOrMore>
      <zeroOrMore>
        <element name="font-alias">
          <attribute name="src">
            <a:documentation>Name of font to alias</a:documentation>
          </attribute>
          <attribute name="dst">
            <a:documentation>Name of font to use in place of source font</a:documentation>
          </attribute>
        </element>
      </zeroOrMore>
    </element>
  </define>
